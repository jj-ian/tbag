#import stdlib

func boolean hasLetterBeenGuessedBefore(string letter) {
	if (guessed[intFromLetter(letter)] == 1) {
		return true;
	} else {
		return false;
	}

}

func boolean isLetterInAnswer(string letter) {
	int i = 0;
	boolean found = false;
	while (i < arrLen(answer) AND found == false) {
		if (answer[i] ~~ letter) {
			found = true;
		} else {}
		i = i + 1;
	}
	return found;
}
