  printLine(1 == 1);
  printLine(99);
  printLine(1 != 2);
  printLine(1 != 1);
  printLine(99);
  printLine(1 < 2);
  printLine(2 < 1);
  printLine(99);
  printLine(1 <= 2);
  printLine(1 <= 1);
  printLine(2 <= 1);
  printLine(99);
  printLine(1 > 2);
  printLine(2 > 1);
  printLine(99);
  printLine(1 >= 2);
  printLine(1 >= 1);
  printLine(2 >= 1);
  finished = true;
}

