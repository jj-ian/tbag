}

boolean finished = false;

func int printLine(int a) {
	print(a);
	print("\n");
	return 0;
}

func int printLine(string s) {
	print(s);
	print("\n");
	return 0;
}

func int printLine(boolean b) {
	print(b);
	print("\n");
	return 0;
}

true {
  printLine(1 + 2);
  printLine(1 - 2);
  printLine(1 * 2);
  printLine(100 / 2);
  printLine(99);
